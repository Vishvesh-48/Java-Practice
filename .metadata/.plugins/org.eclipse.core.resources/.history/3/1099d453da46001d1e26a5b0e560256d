package com.paramterconstructor;

public class Main {
	
	public Main() {
		
		System.out.println("Default Constructor--");
	}
	
	public Main(int i) {
		
		System.out.println("Single parameter Constu--");
		System.out.println(10);
	}
	
	public static void main(String[] args) {
		
		//Main m = new Main();
		Main m1 = new Main(20);
	}

}
