package com.paramter;

public class Demo {
	
  
	public void m1() {
		
		System.out.println("m1 method of Demo class--");
	}

	public void m2(int i) {
		
		System.out.println("single parameter method --");
		System.out.println(i);
	}
	
	public static void main(String[] args) {
		
		Demo d = new Demo();
		d.m1();
		d.m2(10);
	}
}
